{
  "name": "kats-games",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prettier": "prettier  --write './**/*.{js,vue}'",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix"
